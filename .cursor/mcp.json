{
  "mcpServers": {
    "notion": {
      "url": "https://mcp.notion.com/mcp"
    },
    "paperbanana": {
      "command": "uvx",
      "args": [
        "--from",
        "paperbanana[mcp]",
        "paperbanana-mcp"
      ],
      "env": {
        "GOOGLE_API_KEY": "YOUR_GOOGLE_API_KEY"
      }
    }
  }
}